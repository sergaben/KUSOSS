
# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                        controllers.HomeController.index
# Trying connection register kstudents
POST    /signup                  controllers.SignUp.signup
# Log in users
POST    /login                   controllers.Login.login
# Log out user
POST    /logout                  controllers.LogOut.logout
# Insert post into database
POST    /savePost              controllers.Post.savePost
#Getting subjects from database and converting them into scala objects
GET     /getSubjectsNamesAsJson      controllers.getSubjectsController.getSubjectsNamesAsJson
# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

      